{
  "name": "luna-vue-starter",
  "version": "1.0.0",
  "description": "vue ssr template starter",
  "main": "app.js",
  "author": "Djan R Onurer",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-build.json\" pm2 start ecosystem.json $PM2_ARGS",
    "generate-files": "cross-env TS_NODE_PROJECT=\"tsconfig-build.json\" ts-node ./core/build/generate-files.ts",
    "dev": "npm run generate-files && cross-env TS_NODE_PROJECT=\"tsconfig-build.json\" ts-node -r tsconfig-paths/register ./core/build/server.ts",
    "dev:sw": "cross-env TS_NODE_PROJECT=\"tsconfig-build.json\" npm run build:sw && npm run dev",
    "build:sw": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-build.json\" webpack --config ./core/build/webpack.prod.sw.config.ts --mode production --progress",
    "build": "rimraf dist && npm run generate-files && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-build.json\"  webpack --config  core/build/webpack.client.config.ts --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-build.json\"  webpack --config  core/build/webpack.server.config.ts --progress --hide-modules"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "body-scroll-lock": "^2.6.4",
    "compression": "^1.7.1",
    "config": "^3.3.1",
