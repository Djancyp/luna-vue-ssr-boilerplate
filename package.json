    "mini-css-extract-plugin": "^0.9.0",
    "pm2": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.7.1",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.22",
    "vue-google-signin-button": "^1.0.4",
    "vue-lazyload": "^1.3.3",
